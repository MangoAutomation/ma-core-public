  <function>
    <name>systemEventTypeLink</name>
    <function-class>com.serotonin.m2m2.web.taglib.Functions</function-class>
    <function-signature>String systemEventTypeLink(java.lang.String, int, int, javax.servlet.jsp.PageContext)</function-signature>
  </function>
  
  <function>
    <name>auditEventTypeLink</name>
