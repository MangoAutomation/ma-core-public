  <function>
    <name>pointValueTime</name>
    <function-class>com.serotonin.m2m2.web.taglib.Functions</function-class>
    <function-signature>String getTime(com.serotonin.m2m2.rt.dataImage.PointValueTime)</function-signature>
  </function> 
  
  <function>
    <name>padZeros</name>
    <function-class>com.serotonin.m2m2.web.taglib.Functions</function-class>
